DRAW_ENTS=false

lerp=(a,b,t)->(1-t)*a+t*b

-- compare field of 2 tables
comp=(k)->(a,b)->a[k]<b[k]

-- true if object inherits from type
istype=(o,t)->
	c=o.__class
	while c != nil
		if c == t
			return true
		c=c.__parent
	false

class Mtrx3x3
	new:(m={ 1,0,0, 0,1,0, 0,0,1, })=>
		@m=m
		@rl=3
	_index:(r,c)=>r*3+c+1
	get:(r,c)=>
		-- adjust for 1 indexed arrays
		assert r*c <= #@m and r*c >= 0
		@m[@_index(r,c)]
	set:(r,c,v)=>
		assert r*c <= #@m and r*c >= 0
		@m[@_index(r,c)]=v
	_apply:(o,f)=>
		{ a,b,c, d,e,f, g,h,i }=@m
		if "number"== type o
			Mtrx3x3 {
				f(a,o),f(b,o),f(c,o),
				f(d,o),f(e,o),f(f,o),
				f(g,o),f(h,o),f(i,o),
			}
		else
			assert istype o,Mtrx3x3
			{ j,k,l, m,n,o, p,q,r }=o.m
			Mtrx3x3 {
				f(a,j),f(b,k),f(c,l),
				f(d,m),f(e,n),f(f,o),
				f(g,p),f(h,q),f(i,r),
			}
	__add:(o)=>@_apply o,(a,b)->a+b
	__sub:(o)=>@_apply o,(a,b)->a-b
	__mul:(o)=>
		if "number"== type o
			@_apply o,(a,b)->a*b
		else if istype o,Mtrx3x3
			-- computes dot product
			{ a,b,c, d,e,f, g,h,i }=@m
			{ j,k,l, m,n,o, p,q,r }=o.m
			Mtrx3x3 {
				a*j+b*m+c*p,a*k+b*n+c*q,a*l+b*o+c*r,
				d*j+e*m+f*p,d*k+e*n+f*q,d*l+e*o+f*r,
				g*j+h*m+i*p,g*k+h*n+i*q,g*l+h*o+i*r,
			}
	__div:(o)=>@_apply o,(a,b)->a/b
	__idiv:(o)=>@_apply o,(a,b)->a//b
	__unm:=>@__mul -1
	__tostring:=>
		{ a,b,c, d,e,f, g,h,i, }=@m
		"[#{a},#{b},#{c}, #{d},#{e},#{f}, #{g},#{h},#{i}]"

class Transform
	new:=>
		@s={}
		@r={}
		@t={}
	translate:(x=0,y=0)=>
		table.insert @t,Mtrx3x3 {
			1,0,x,
			0,1,y,
			0,0,1,
		}
	scale:(w=1,h=1)=>
		table.insert @s,Mtrx3x3 {
			w,0,0,
			0,h,0,
			0,0,1,
		}
	reflect:=>@scale -1,-1
	reflect_x:=>@scale 1,-1
	reflect_y:=>@scale -1,1
	shear:(x=0,y=0)=>
		table.insert @t,Mtrx3x3 {
			1,x,0,
			y,1,0,
			0,0,1,
		}
	rotate:(ang=0)=>
		c,s=@__angcossin ang
		table.insert @t,Mtrx {
			c,-s,0,
			s, c,0,
			0, 0,1,
		}
	rotate_around_axis:(ang=0,x=0,y=0)=>
		table.insert @t,@translate(-x,-y) *
			@rotate(ang) *
			@translate(x,y)
	apply:(m)=>
		for s in *@s
			m*=s
		for r in *@r
			m*=r
		for t in *@t
			m*=t

class Vec2 extends Mtrx3x3
	new:(x=0,y=0)=>
		super {
			x,0,0,
			y,0,0,
			1,0,0,
		}
	from:(mtrx)=>
		assert istype mtrx,Mtrx3x3
		Vec2 mtrx\get(0,0),mtrx\get(1,0)
	x:(v)=>
		if v
			@\set 0,0,v
		else
			@\get 0,0
	y:(v)=>
		if v
			@\set(1,0,v)
		else
			@\get(1,0)
	__add:(v)=>@from super(v)
	__sub:(v)=>@from super(v)
	-- computes dot product
	__mul:(v)=>@from super(v)
	__unm:=>@from super(v)
	__tostring:=>"[#{@x!}, #{@y!}]"
	__len:=>math.abs math.sqrt(@x!*@x!+@y!*@y!)
	magnitude:=>_len!
	clamp:(min,max)=>
		Vec2 math.min(math.max(@x!,min.x!),max.x!),math.min(math.max(@y!,min.y!),max.y!)
	distance:(v)=>
		dx=@x!-v\x!
		dy=@y!-v\y!
		math.abs math.sqrt(dx*dx+dy*dy)
	normalize:()=> Vec2 @x!/#@,@y!/#@

class Behavior
	new:(o={})=>
		@order=o.order or 0
	update:(game,scene,entity)=>

class Player extends Behavior
	new:(o={})=>
		super o
	update:(game,scene,entity)=>
		vx=entity.vel\x!
		if entity.vel\x!>-entity.vel_max\x! and btn 2
			entity.vel\x entity.vel\x!-entity.acc\x!
			entity.flip=1
			if entity.mode=="idle"
				entity.mode="walk"
		if entity.vel\x!<entity.vel_max\x! and btn 3
			entity.vel\x entity.vel\x!+entity.acc\x!
			entity.flip=0
			if entity.mode=="idle"
				entity.mode="walk"
		if vx==entity.vel\x!
			if entity.vel\x!>0.5
				entity.vel\x entity.vel\x!-entity.acc\x!
			else if entity.vel\x!<-0.5
				entity.vel\x entity.vel\x!+entity.acc\x!
			else
				entity.vel\x 0

		if scene\hit_wall entity
			entity.vel\x 0

		if scene\hit_ground entity
			entity.vel\y 0
			if entity.mode=="jump"
				entity.mode="idle"
		else
			entity.vel\y entity.vel\y!+entity.acc\y!

		if entity.vel\y! ==0 and btnp 4
			entity.vel\y entity.vel_max\y!
			entity.mode="jump"

		if scene\hit_ceiling entity
			entity.vel\y 0

		if entity.vel\x! ==0 and entity.vel\y! ==0
			entity.mode="idle"

		entity.pos\x entity.pos\x!+entity.vel\x!
		entity.pos\y entity.pos\y!+entity.vel\y!

		super game,scene,entity

class Entity
	new:(o={})=>
		@pos=o.pos or Vec2!
		@vel=o.vel or Vec2!
		@vel_max=o.vel_max or Vec2 1.5,1.5
		@acc=o.acc or Vec2 0.3,0.3
		@z_index=o.z_index or 0
		@w=o.w or 8
		@h=o.h or 8
		@behaviors=o.behaviors or {}
		@damage=1
		@hp=3
		for b in *@behaviors
			assert istype b,Behavior
	-- called once per entity on first update
	onload:(game,scene)=>
	take_damage:(game,scene,enemy)=>
		trace "ouch"
		@hp-=enemy.damage
		if @hp < 0
			game.game_end=true
	update:(game,scene)=>
		if not @loaded
			@onload game,scene
			@loaded=true
		table.sort @behaviors,comp("order")
		for b in *@behaviors
			b\update game,scene,@
	draw:(game,scene)=>
		if DRAW_ENTS
			rectb @pos.x,@pos.y,@w,@h,11
			print "x:#{@x} y:#{@y}",@x,@y+@h,11
			print "vx:#{@vx} vy:#{@vy}",@x,@y+@h+7,11

class Sprite
	new:(o={})=>
		@id=o.id or 0
		@colorkey=o.colorkey or -1
		@w=o.w or 1
		@h=o.h or 1
		@scale=o.scale or 1
		@flip=o.flip or 0
		@rotate=o.rotate or 0
	draw:(game,scene,entity)=>
		spr @id,
			entity.x,entity.y,
			@colorkey,
			entity.scale or @scale,
			entity.flip or @flip,
			entity.rotate or @rotate,
			@w,@h

class SpriteGroup
	new:(o={})=>
		@sprites=o.sprites or {}
		for s in *@sprites
			assert istype(s,Sprite) or istype(s,SpriteGroup)
	draw:(game,scene,entity)=>
		for s in *@sprites
			s\draw game,scene,entity

class Frame extends SpriteGroup
	new:(o={})=>
		super o
		@hold=o.hold or 1

class Transform extends SpriteGroup
	new:(o={})=>
		super o
		@x=o.x or 0
		@y=o.y or 0
		@fx=o.fx or 0
	draw:(game,scene,entity)=>
		entity = if entity.flip == 1
			{
				x:@fx+entity.pos\x!
				y:@y+entity.pos\y!
				scale:entity.scale
				flip:entity.flip
				rotate:entity.rotate
			}
		else
			{
				x:@x+entity.pos\x!
				y:@y+entity.pos\y!
				scale:entity.scale
				flip:entity.flip
				rotate:entity.rotate
			}
		if @flip
			@child.flip=@flip
		super game,scene,entity

class Mode
	new:(o={})=>
		@frames=o.frames or {}
		for f in *@frames
			assert istype f,Frame

class Actor extends Entity
	new:(o={})=>
		super o
		@isplayer=o.isplayer
		@sprite=o.sprite
		@scale=o.scale
		@flip=o.flip
		@rotate=o.rotate
		@mode=o.mode or "idle"
		@modes=o.modes or {}
		-- Points at the current frame to display.
		@frame_cursor=1
		-- The next tic to update on.
		@frame_next_tic=0
	update:(game,scene)=>
		super game,scene
		@prev_mode=@mode

		m_data=@modes[@mode]
		assert m_data,"not a valid mode: #{@mode}"
		if @mode != @prev_mode
			@frame_cursor=0
		if game.t >= @frame_next_tic or @mode != @prev_mode
			@frame_cursor+=1
			if @frame_cursor > #m_data.frames
				@frame_cursor=1
				if m_data.once
					-- don't repeat one time actions
					@mode="idle"
			@sprite=m_data.frames[@frame_cursor]
			@frame_next_tic=game.t+@sprite.hold
	draw:(game,scene)=>
		@sprite\draw game,scene,@
		super game,scene

class Map extends Entity
	new:(o={})=>
		super o
		@w=o.w or 30
		@h=o.h or 17
		@sx=o.sx or 0
		@sy=o.sy or 0
		@colorkey=o.colorkey or -1
		@scale=o.scale or 1
		@remap=o.remap
	update:(game,scene)=>
		super game,scene
	draw:(game,scene)=>
		super game,scene
		map @x,@y,@w,@h,@sx,@sy,
			@colorkey,@scale,
			@remap

class Scene extends Entity
	new:(o={})=>
		super o
		@cls_color=o.cls_color or 0
		@entities=o.entities or {}
		for e in *@entities
			assert istype e,Entity
	issolid:(x,y)=>fget mget(x//8,y//8),0
	_aabb:(o)=>
		x=o.pos\x!
		trace x
		y=o.pos\y!
		trace y
		vx=o.vel\x!
		trace vx
		vy=o.vel\y!
		trace vy
		x+vx,y+vy,x+vx+o.w-1,y+vy+o.h-1
	hit_wall:(entity)=>
		x1,y1,x2,y2=@_aabb entity
		x = if entity.flip == 1
			x1
		else
			x2
		for y=y1,y2-entity.vel\y!
			if @issolid(x,y)
				return true
	hit_ground:(entity)=>
		x1,_,x2,y2=@_aabb entity
		for x=x1,x2
			if @issolid(x,y2+1)
				return true
	hit_ceiling:(entity)=>
		x1,y1,x2,_=@_aabb entity
		for x=x1,x2
			if @issolid(x,y1)
				return true
	add_entity:(e)=>
		assert istype e,Entity
		table.insert @entities,e
	update:(game)=>
		table.sort @entities,comp("z_index")
		for entity in *@entities
			entity\update game,@
	draw:(game)=>
		cls @cls_color
		for entity in *@entities
			entity\draw game,@

class Game
	new:(o={})=>
		@scenes=o.scenes or {}
		for s in *@scenes
			assert istype s,Scene
		@cur_scene=1
		@t=0
	add_scene:(s)=>
		assert istype s,Scene
		table.insert @scenes,s
	update:=>
		if @game_end
			return
		@scenes[@cur_scene]\update @
		@t+=1
	draw:=>
		@scenes[@cur_scene]\draw @
		if @game_end
			cls!
			print "You are dead.",64,84,1,false,2

