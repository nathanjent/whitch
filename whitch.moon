-- compare field of 2 tables
comp=(k)->(a,b)->a[k]<b[k]

lerp=(a,b,t)->(1-t)*a+t*b

-- true if object inherits from type
istype=(o,t)->
	c=o.__class
	while c != nil
		if c == t
			return true
		c=c.__parent
	false

class Vec
	new:(x=0,y=0)=>
		@x=x
		@y=y
	__add:(v)=>@_apply v,(a,b)->a+b
	__sub:(v)=>@_apply v,(a,b)->a-b
	__div:(v)=>@_apply v,(a,b)->a/b
	__idiv:(v)=>@_apply v,(a,b)->a//b
	__mod:(v)=>@_apply v,(a,b)->a%b
	__mul:(v)=>
		if "number"==type v
			@_apply v,(a,b)->a*b
		else if istype v,Vec
			-- dot product
			@x*v.x+@y*v.y
	__unm:=>@ * -1
	_apply:(v,f)=>
		if "number"== type v
			Vec f(@x,v),f(@y,v)
		else if istype v,Vec
			Vec f(@x,v.x),f(@y,v.y)
	__eq:(v)=>istype(v,Vec) and
		@x==v.x and @y==v.y
	__tostring:=>"[x:#{@x} y:#{@y}]"
	lensq:=>@x*@x+@y*@y
	__len:=>
		if @x == 0
			@y
		else if @y == 0
			@x
		else
			math.sqrt(@lensq!)
	clamp:(min,max)=>
		Vec math.min(math.max(@x,min.x),max.x),
			math.min(math.max(@y,min.y),max.y)
	distance:(v)=>math.abs #(@-v)
	norm:()=> Vec @x/#@,@y/#@
	rotate:(a)=>
		a=math.rad a
		s,c=math.sin(a),math.cos(a)
		Vec @x*c-@y*s,@x*s+@y*c
	rotate_around_axis:(a,pos)=>
		(@+pos)\rotate a
	angle_atan:(v)=>
		math.deg math.atan2(#v,#@)
	angle_cos:(v)=>
		math.deg math.cos(@*v/(#@*#v))

class Behavior
	new:(o={})=>
		@order=o.order or 0
	update:(game,scene,actor)=>

class Player extends Behavior
	new:(o={})=>
		super o
	update:(game,scene,actor)=>
		vx=actor.vel.x
		if actor.vel.x>-actor.vel_max.x and btn 2
			actor.vel.x-=actor.acc.x
			actor.flip=1
			if actor.mode=="idle"
				actor.mode="walk"
		if actor.vel.x<actor.vel_max.x and btn 3
			actor.vel.x+=actor.acc.x
			actor.flip=0
			if actor.mode=="idle"
				actor.mode="walk"
		if vx==actor.vel.x
			if actor.vel.x>0.5
				actor.vel.x-=actor.acc.x
			else if actor.vel.x<-0.5
				actor.vel.x+=actor.acc.x
			else
				actor.vel.x=0

		if scene\hit_wall actor
			actor.vel.x=0

		if scene\hit_ground actor
			actor.vel.y=0
			if actor.mode=="jump"
				actor.mode="idle"
		else
			actor.vel.y+=actor.acc.y

		if actor.vel.y==0 and btnp 4
			actor.vel.y=actor.vel_max.y
			actor.mode="jump"

		if scene\hit_ceiling actor
			actor.vel.y=0

		if actor.vel.x==0 and actor.vel.y==0
			actor.mode="idle"

		actor.pos.x+=actor.vel.x
		actor.pos.y+=actor.vel.y

		super game,scene,actor

class Ouch extends Behavior
	new:(o={})=>
		super o
	update:(game,actor)=>
		if actor.is_player and actor.is_hurt
			game.camera.shake=true
			game.shake=2
			actor.is_hurt=false

class FlipX extends Behavior
	new:(o={})=>
		super o
	update:(game,actor)=>
		if btn 2
			actor.flip=0
		if btn 3
			actor.flip=1

class Behavioral
	new:(o={})=>
		@behaviors=o.behaviors or {}
		for b in *@behaviors
			assert istype b,Behavior
	update:(game,scene)=>
		table.sort @behaviors,comp("order")
		for b in *@behaviors
			b\update game,scene,@
	draw:(game,scene)=>

class Actor extends Behavioral
	new:(o={})=>
		super o
		@pos=o.pos or Vec o.x,o.y
		@vel=o.vel or Vec o.vx,o.vy
		@vel_max=o.vel_max or Vec o.vx_max or 1.5,o.vy_max or 1.5
		@acc=o.acc or Vec o.ax or 0.3,o.ay or 0.3
		@z_index=o.z_index or 0
		@origin=Vec!
		@w=o.w or 8
		@h=o.h or 8
		@damage=1
		@is_player=o.is_player
		@hp=3
	-- called once per actor on first update
	update:(game,scene)=>
		if not @loaded
			@onload game,scene
			@loaded=true
		super game,scene
	draw:(game,scene)=>
		if DEBUG
			x,y,w,h=@hitbox!
			--trace "actor orig:#{@origin} pos:#{@pos}"
			rectb x,y,w,h,11
			print "x:#{@pos.x} y:#{@pos.y}",x+w,y+h,11,0,1,1
			print "vx:#{@vel.x} vy:#{@vel.y}",x+w,y+h+7,11,0,1,1
	hitbox:=>
		@pos.x-@w/2,@pos.y-@h/2,@w,@h
	aabb:=>
		w,h=@w/2,@h/2
		@pos.x-w,@pos.y-h,@pos.x+w,@pos.y+h
	onload:(game,scene)=>
	take_damage:(game,scene,enemy)=>
		trace "ouch"
		@hp-=enemy.damage
		if @hp < 0
			game.game_end=true

class Sprite
	new:(o={})=>
		@id=o.id or 0
		@x=0
		@y=0
		@colorkey=o.colorkey or -1
		@w=o.w or 1
		@h=o.h or 1
		@z_index=o.z_index or 0
		@scale=o.scale or 1
		@flip=o.flip or 0
		@rotate=o.rotate or 0
	update:(game,scene,actor,parent)=>
		@flip=parent.flip
		p=parent.origin+parent.pos
		@x=p.x
		@y=p.y
	draw:(game,scene,actor,parent)=>
		spr @id,
			@x-@w*4,@y-@h*4,
			@colorkey,
			@scale,
			@flip,
			@rotate,
			@w,@h
		--if DEBUG
			--print "#{@}",@x+@w*8,@y+@h*8,11,0,1,1
	__tostring:=>
		"id: #{@id} x:#{@x} y:#{@y} z:#{@z_index} flip:#{@flip}"

class SpriteSet extends Sprite
	new:(o={})=>
		super o
		@pos=o.pos or Vec o.x,o.y
		@origin=Vec!
		@sprites=o.sprites or {}
		for s in *@sprites
			assert istype s,Sprite
	update:(game,scene,actor,parent)=>
		@origin=parent.origin+parent.pos
		if @flip != parent.flip
			@flip=parent.flip
			@pos=Vec @pos.x*-1,@pos.y
		table.sort @sprites,comp 'z_index'
		for s in *@sprites
			s\update game,scene,actor,@
	draw:(game,scene,actor,parent)=>
		for s in *@sprites
			s\draw game,scene,actor,@
		if DEBUG
			--pix @origin.x,@origin.y,4
			--circb @origin.x,@origin.y,2,4
			--trace "set o:#{@origin} p:#{@pos}"
			line @origin.x,@origin.y,@origin.x+@pos.x,@origin.y+@pos.y,4

class Frame extends SpriteSet
	new:(o={})=>
		super o
		@hold=o.hold or 1

class Mode
	new:(o={})=>
		@frames=o.frames or {}
		for f in *@frames
			assert istype f,Frame

class ModalActor extends Actor
	new:(o={})=>
		super o
		@sprite=o.sprite
		@scale=o.scale
		@flip=o.flip or 0
		@rotate=o.rotate
		@mode=o.mode or "idle"
		@modes=o.modes or {}
		-- Points at the current frame to display.
		@frame_cursor=1
		-- The next tic to update on.
		@frame_next_tic=0
	update:(game,scene)=>
		super game,scene

		m_data=@modes[@mode]
		assert m_data,"not a valid mode: #{@mode}"
		if @mode != @prev_mode
			@frame_cursor=0
		if game.t >= @frame_next_tic or @mode != @prev_mode
			@frame_cursor+=1
			if @frame_cursor > #m_data.frames
				@frame_cursor=1
				if m_data.once
					-- don't repeat one time actions
					@mode="idle"
			@sprite=m_data.frames[@frame_cursor]
			@frame_next_tic=game.t+@sprite.hold
		@sprite\update game,scene,@,@
		@prev_mode=@mode
	draw:(game,scene)=>
		@sprite\draw game,scene,@,@
		super game,scene

class Map extends Behavioral
	new:(o={})=>
		super o
		@w=o.w or 30
		@h=o.h or 17
		@sx=o.sx or 0
		@sy=o.sy or 0
		@colorkey=o.colorkey or -1
		@scale=o.scale or 1
		@remap=o.remap
	update:(game,scene)=>
		super game,scene,@
	draw:(game,scene)=>
		map @x,@y,@w,@h,@sx,@sy,
			@colorkey,@scale,
			@remap

class Scene
	new:(o={})=>
		@cls_color=o.cls_color or 0
		@actors=o.actors or {}
		for a in *@actors
			assert istype a,Actor
		@maps=o.maps or {}
		for m in *@maps
			assert istype m,Map
	update:(game)=>
		for m in *@maps
			m\update game,@
		table.sort @actors,comp("z_index")
		for a in *@actors
			a\update game,@
	draw:(game)=>
		cls @cls_color
		for m in *@maps
			m\draw game,@
		for a in *@actors
			a\draw game,@
	issolid:(x,y)=>fget mget(x//8,y//8),0
	hit_wall:(actor)=>
		x1,y1,x2,y2=actor\aabb!
		x = if actor.flip == 1
			x1
		else
			x2
		for y=y1,y2-actor.vel.y
			if @issolid(x+actor.vel.x,y+actor.vel.y)
				return true
	hit_ground:(actor)=>
		x1,_,x2,y2=actor\aabb!
		for x=x1,x2
			if @issolid(x+actor.vel.x,y2+actor.vel.y+1)
				return true
	hit_ceiling:(actor)=>
		x1,y1,x2,_=actor\aabb!
		for x=x1,x2
			if @issolid(x+actor.vel.x,y1+actor.vel.y)
				return true
	add_actor:(e)=>
		assert istype e,Actor
		table.insert @actors,e

class Game
	new:(o={})=>
		@scenes=o.scenes or {}
		for s in *@scenes
			assert istype s,Scene
		@cur_scene=1
		@t=0
		@camera=o.camera or {}
		@shake=0
	update:=>
		if @game_end
			return
		@scenes[@cur_scene]\update @
		@t+=1
	draw:=>
		@scenes[@cur_scene]\draw @
		if @game_end
			cls!
			print "You are dead.",64,84,1,false,2
		@camera_shake!
	add_scene:(s)=>
		assert istype s,Scene
		table.insert @scenes,s
	camera_shake:=>
		if @camera.shake
			if @shake>0
				d=1
				poke 0x3FF9,math.random(-d,d)
				poke 0x3FF9+1,math.random(-d,d)
				@shake-=1
				if @shake==0
					memset 0x3FF9,0,2
					@camera.shake=false

